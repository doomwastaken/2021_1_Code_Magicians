{
  "name": "pinterest",
  "version": "1.0.0",
  "main": "server/index.js",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "pm2": "^4.5.5",
    "nodemon": "^2.0.7",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "handlebars": "^4.7.7",
    "husky": "^5.1.1"
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "sudo pm2 start nodemon server/index.js ",
    "stop": "sudo pm2 stop nodemon",
    "lint": "npx -s eslint src/",
    "fix": "npx -s eslint src/ --fix",
    "compile_hbs": "npx handlebars src/views/*/*.hbs -f src/assets/js/handlebars.precompiled.js"
  }
}
