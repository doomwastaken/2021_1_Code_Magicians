{
  "name": "pinterest",
  "version": "1.0.0",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "globstar": "^1.0.0",
    "handlebars": "^4.7.7",
    "husky": "^5.1.1",
    "sass": "^1.32.8",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "start": "node server.js",
    "prestart": "npm run compile_hbs",
    "postinstall": "husky install",
    "lint": "npx eslint src/",
    "compile_hbs": "globstar -- npx handlebars src/**/*.hbs -f src/assets/js/handlebars.precompiled.js"
  }
}
