<div class="vlist-wrapper">
    {{#each cols}}
        <div class="vlist-col" style="width:{{../colWidth}}px">
        {{#each pins}}
            <div class="vlist-element" style="height:{{this.imageHeight}}px; background-color:#{{this.imageAvgColor}};">
                <img src="{{this.imageLink}}" alt="{{this.title}}" class="vlist-element__media" style="" loading="lazy">
                <a href="/pin/{{this.ID}}" class="vlist-element__link"> {{this.userID}} </a>
            </div>
        {{/each}}
        </div>
    {{/each}}
</div>


